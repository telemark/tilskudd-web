{
  "name": "tilskudd-web",
  "description": "Løsning for søknader om tilskudd",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "node standalone.js",
    "dev": "NODE_ENV=development node standalone.js"
  },
  "keywords": [
    "tilskudd",
    "søknadsskjema",
    "microsite",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/tilskudd.t-fk.no.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/tilskudd.t-fk.no/issues"
  },
  "louie": {
    "versionName": "Australian Crawl",
    "versionVideoUrl": "https://www.youtube.com/watch?v=URgPL9AjkbQ",
    "systemName": "tilskudd.t-fk.no"
  },
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "nsp": "2.7.0",
    "standard": "10.0.3",
    "tap": "10.3.2"
  },
  "dependencies": {
    "axios": "0.16.2",
    "blankie": "2.1.0",
    "good": "7.3.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.2",
    "handlebars": "4.0.10",
    "hapi": "16.5.2",
    "hapi-auth-cookie": "7.0.0",
    "hapi-auth-jwt2": "7.3.0",
    "inert": "4.2.1",
    "jsonwebtoken": "7.4.3",
    "scooter": "4.0.0",
    "simple-encryptor": "1.1.0",
    "vision": "4.1.1",
    "winston": "2.3.1",
    "winston-papertrail": "1.0.5",
    "yar": "8.1.2"
  }
}
