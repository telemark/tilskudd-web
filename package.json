{
  "name": "tilskudd-web",
  "description": "Løsning for søknader om tilskudd",
  "version": "1.0.5",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.11.1"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "node standalone.js",
    "dev": "NODE_ENV=development node standalone.js",
    "now-deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "tilskudd",
    "søknadsskjema",
    "microsite",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/tilskudd-web.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tilskudd-web/issues"
  },
  "homepage": "https://github.com/telemark/tilskudd-web#readme",
  "now": {
    "alias": [
      "tilskudd.tjeneste.win"
    ],
    "type": "npm",
    "dotenv": "production.env",
    "engines": {
      "node": "8.11.1"
    }
  },
  "louie": {
    "versionName": "Australian Crawl",
    "versionVideoUrl": "https://www.youtube.com/watch?v=URgPL9AjkbQ",
    "systemName": "tilskudd.t-fk.no"
  },
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "coveralls": "3.0.1",
    "nsp": "3.2.1",
    "standard": "11.0.1",
    "tap": "11.1.4"
  },
  "dependencies": {
    "axios": "0.18.0",
    "blankie": "3.0.1",
    "handlebars": "4.0.11",
    "hapi": "16.6.3",
    "hapi-auth-cookie": "7.0.0",
    "hapi-auth-jwt2": "7.4.1",
    "inert": "4.2.1",
    "jsonwebtoken": "8.2.1",
    "scooter": "5.0.0",
    "simple-encryptor": "1.2.0",
    "vision": "4.1.1",
    "winston": "2.4.2",
    "winston-papertrail": "1.0.5",
    "yar": "8.1.2"
  }
}
